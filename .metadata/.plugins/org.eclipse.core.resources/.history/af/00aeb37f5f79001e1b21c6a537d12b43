package Disarium_Number;

public interface Test {
	public static void disariumCheck(int number)
	{
		int temporary=number;
		int digit=0;
		while(temporary!=0)
		{
			temporary/=10;
			digit++;
		}
		temporary=number;
		int sum=0;
		for(int i=digit;i>0;i++)
		{
			sum+=raise(temporary%10,digit);
			digit--;
		}
		
		
	}
	public static int raise(int num,int digit)
	{
		for(int i=0;i<num;i++)
		{
			num*=num;
		}
		return num;
	}

}
